{
  "name": "dart-client-graphql-codegen",
  "version": "0.1.0",
  "description": "GraphQL Code Generator plugin for generating Dart types",
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.ts",
    "test": "jest --no-watchman",
    "build": "tsc && rename 's/.js/.cjs/' dist/*.js"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^2.1.1",
    "@graphql-codegen/schema-ast": "^2.1.0",
    "@graphql-codegen/visitor-plugin-common": "2.2.0",
    "auto-bind": "~4.0.0",
    "tslib": "~2.3.0"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "^1.17.7",
    "@types/jest": "^27.0.1",
    "jest": "^27.1.0",
    "ts-jest": "^27.0.5"
  }
}
